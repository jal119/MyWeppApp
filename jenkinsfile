pipeline {
    agent {
        docker {
            image 'maven'       
        }
    }
    
    stages {
                sh 'mvn --version'
            }
        }
        
        stage('Build') {
            steps {
                sh 'echo build'
                  
                }
            }
            steps {
                sh 'node -version'
            }
        }
    }
}

